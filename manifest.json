{
  "manifest_version": 3,
  "name": "QuoteFlow",
  "description": "Quick access to QuoteFlow full website",
  "version": "1.0",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_title": "Open QuoteFlow"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs"
  ]
}